<main>
<section>
	<h1>All Users</h1>

	<%= form_tag(users_path, :method => "get", id: "search-form") do %>
	<%= text_field_tag :search, params[:search], placeholder: "Search Users" %>
	<%= submit_tag "Search" %>
	<% end %>

	<ul>
		<% @users.each do |user| %>
			<li><%= link_to user.username, user %></li>
		<% end %>
	</ul>
</section>
</main>